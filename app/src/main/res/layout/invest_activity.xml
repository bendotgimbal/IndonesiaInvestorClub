                android:textColor="@color/colorPrimary"
                android:textSize="14sp"
                />

            <TextView
                android:layout_width="80dp"
                android:gravity="center"
                android:layout_height="50dp"
                android:padding="5dp"
                android:background="@drawable/background_table"
